REDIS_URL=redis://localhost:6381

# Test TAC Configuration
TAC_SDK_ENDPOINT=https://test-api.tac.build
TAC_API_KEY=test_tac_api_key
TAC_WEBHOOK_SECRET=test_webhook_secret
